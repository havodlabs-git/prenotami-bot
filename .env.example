# Configuração do Bot PrenotaMI
# Copie este arquivo para .env e preencha com suas credenciais

# Credenciais de acesso ao PrenotaMI
PRENOTAMI_EMAIL=seu_email@exemplo.com
PRENOTAMI_PASSWORD=sua_senha_aqui

# Token do Bot Discord (opcional - apenas se for usar o bot do Discord)
DISCORD_BOT_TOKEN=seu_token_do_discord_aqui

# ID do usuário Discord autorizado (opcional - deixe vazio para permitir todos)
DISCORD_USER_ID=

# Configurações do bot
CHECK_INTERVAL_MINUTES=30
HEADLESS_MODE=false
